#include<stdio.h>
#inlcude<stdlib.h>
#include<time.h>
int main()
{
int numb9er,guess,attempts=0;
srand(time(0));
number = rand()%100+1;
printf("welcome to Number Guessing Game!\n");
printff("Guess a number between 1 to 100");
do{
printf("Enter your guess");
scanf("%d",&guess);
attempts++;
if(guess>number)
  printf("Too High!\n");
else if
  printf("Too Low!\n");
else
  printf("Congratulations! You guessed it in %d  attempts.\n",attempts);
}
while(guess !=number);
return 0;
}
